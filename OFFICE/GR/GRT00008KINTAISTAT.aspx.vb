                        WW_TEXT = WW_TEXT & Mid(StrConv(T00007row("STAFFNAMES"), VbStrConv.Wide).PadRight(10, "　"), 1, 10)
                    End If

                    '９１．予備
                    WW_TEXT = WW_TEXT & Space(2)

                    '改行
                    WW_TEXT = WW_TEXT & ControlChars.NewLine

                    OutFile.Write(WW_TEXT)

                Next

                'ファイルクローズ
            End Using

        Catch ex As Exception
            Master.output(C_MESSAGE_NO.FILE_IO_ERROR, C_MESSAGE_TYPE.ABORT, "File Output Error")
            CS0011LOGWRITE.INFSUBCLASS = "MAIN"                         'SUBクラス名
            CS0011LOGWRITE.INFPOSI = "File Output Error"                '
