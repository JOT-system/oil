    ''' <summary>
    ''' エラー情報出力
    ''' </summary>
    ''' <param name="iProcKbn">1:NGデータ,2:未申請データ,3:未承認データ</param>
    ''' <param name="iLimitYM"></param>
    ''' <param name="iHorg"></param>
    ''' <param name="iHorgNm"></param>
    ''' <param name="oRtn"></param>
    ''' <remarks></remarks>
    Protected Sub ErrInfoOut(ByVal iProcKbn As String, ByVal iLimitYM As String, ByVal iHorg As String, ByVal iHorgNm As String, ByRef oRtn As String)

        oRtn = C_MESSAGE_NO.NORMAL

        Try
            Dim T7tbl As DataTable = New DataTable
            'カラム設定
            T7tbl.Columns.Add("STAFFCODE", GetType(String))                      '従業員コード
            T7tbl.Columns.Add("STAFFNAME", GetType(String))                      '従業員名称
            T7tbl.Columns.Add("WORKDATE", GetType(Date))                         '計上日付

            Dim SQLcon = CS0050SESSION.getConnection
            Dim SQLcmd As New SqlCommand
            SQLcon.Open() 'DataBase接続(Open)

            Dim SQLStr As String = ""
            Dim WHEREStr As String = ""

            SQLStr =
                     "SELECT " _
                   & "       isnull(rtrim(T7.STAFFCODE),'') as STAFFCODE                    " _
                   & "      ,isnull(rtrim(MB1.STAFFNAMES),'') as STAFFNAME                  " _
                   & "      ,isnull(rtrim(T7.WORKDATE),'') as WORKDATE                      " _
                   & "  FROM T0007_KINTAI T7                                                "

            If iProcKbn = "1" Then
                'NGレコード
                WHEREStr =
                     "  LEFT JOIN MB001_STAFF MB1                                           " _
                   & "    ON MB1.CAMPCODE  = T7.CAMPCODE                                    " _
                   & "   and MB1.STAFFCODE = T7.STAFFCODE                                   " _
                   & "   and MB1.STYMD    <= @NOW                                           " _
                   & "   and MB1.ENDYMD   >= @NOW                                           " _
                   & "   and MB1.DELFLG   <> '1'                                            " _
                   & " WHERE T7.CAMPCODE  = @CAMPCODE                                       " _
                   & "   and T7.TAISHOYM  = @LIMITYM                                        " _
                   & "   and T7.HDKBN     = 'H'                                             " _
                   & "   and T7.RECODEKBN = '0'                                             " _
                   & "   and T7.HORG      = @HORG                                           " _
                   & "   and ((T7.STDATE   = T7.ENDDATE                                     " _
                   & "   and   T7.STTIME  <> '00:00'                                        " _
                   & "   and   T7.ENDTIME  = '00:00')                                       " _
                   & "    or   (T7.STDATE   = T7.ENDDATE                                    " _
                   & "   and   T7.STTIME   = '00:00'                                        " _
                   & "   and   T7.ENDTIME <> '00:00'))                                      " _
                   & "   and T7.DELFLG   <> '1'                                             "
            End If

            If iProcKbn = "2" Then
                '未申請（否認も含む）
                WHEREStr =
                     "  LEFT JOIN T0009_APPROVALHIST T9                                     " _
                   & "    ON T9.CAMPCODE  = T7.CAMPCODE                                     " _
                   & "   and T9.APPLYID   = T7.APPLYID                                      " _
                   & "   and T9.DELFLG   <> '1'                                             " _
                   & "  LEFT JOIN MB001_STAFF MB1                                           " _
                   & "    ON MB1.CAMPCODE  = T7.CAMPCODE                                    " _
                   & "   and MB1.STAFFCODE = T7.STAFFCODE                                   " _
                   & "   and MB1.STYMD    <= @NOW                                           " _
                   & "   and MB1.ENDYMD   >= @NOW                                           " _
                   & "   and MB1.DELFLG   <> '1'                                            " _
                   & " WHERE T7.CAMPCODE  = @CAMPCODE                                       " _
                   & "   and T7.TAISHOYM  = @LIMITYM                                        " _
                   & "   and T7.HDKBN     = 'H'                                             " _
                   & "   and T7.RECODEKBN = '0'                                             " _
                   & "   and T7.HORG      = @HORG                                           " _
                   & "   and T7.RIYU     <> ''                                              " _
                   & "   and T7.DELFLG   <> '1'                                             " _
                   & "   and isnull(T9.STATUS,'09')   = '09'                                "
            End If

            If iProcKbn = "3" Then
                'NGレコード
                WHEREStr =
                     "  LEFT JOIN MB001_STAFF MB1                                           " _
                   & "    ON MB1.CAMPCODE  = T7.CAMPCODE                                    " _
                   & "   and MB1.STAFFCODE = T7.STAFFCODE                                   " _
                   & "   and MB1.STYMD    <= @NOW                                           " _
                   & "   and MB1.ENDYMD   >= @NOW                                           " _
                   & "   and MB1.DELFLG   <> '1'                                            " _
                   & " WHERE 1 = 0                                                          "
            End If


            SQLcmd = New SqlCommand(SQLStr & WHEREStr, SQLcon)
            Dim P_CAMPCODE As SqlParameter = SQLcmd.Parameters.Add("@CAMPCODE", System.Data.SqlDbType.NVarChar)
            Dim P_LIMITYM As SqlParameter = SQLcmd.Parameters.Add("@LIMITYM", System.Data.SqlDbType.NVarChar)
            Dim P_HORG As SqlParameter = SQLcmd.Parameters.Add("@HORG", System.Data.SqlDbType.NVarChar)
            Dim P_NOW As SqlParameter = SQLcmd.Parameters.Add("@NOW", System.Data.SqlDbType.Date)
            P_CAMPCODE.Value = work.WF_SEL_CAMPCODE.Text
            P_LIMITYM.Value = iLimitYM
            P_HORG.Value = iHorg
            P_NOW.Value = Date.Now

            SQLcmd.CommandTimeout = 300
            Dim SQLdr As SqlDataReader = SQLcmd.ExecuteReader()

            T7tbl.Load(SQLdr)
            If T7tbl.Rows.Count > 0 Then
                If iProcKbn = "1" Then
                    rightview.addErrorReport("・不完全な勤務データが存在します。")
                ElseIf iProcKbn = "2" Then
                    rightview.addErrorReport("・未申請勤務データが存在します。")
                End If
            End If
            If iProcKbn = "3" Then
                rightview.addErrorReport("・未承認データが存在します。")
                rightview.addErrorReport("  --> 承認者（代行者）に連絡してください。 ,")
            End If

            For Each T7row As DataRow In T7tbl.Rows
                rightview.addErrorReport("  --> 部署    ： " & iHorgNm & "(" & iHorg & ") ,")
                rightview.addErrorReport("  --> 日付    ： " & T7row("WORKDATE") & " ,")
                rightview.addErrorReport("  --> 従業員  ： " & T7row("STAFFNAME") & "(" & T7row("STAFFCODE") & ") ,")
            Next

            SQLdr.Dispose() 'Reader(Close)
            SQLdr = Nothing
            SQLcmd.Dispose()
            SQLcmd = Nothing
            SQLcon.Close() 'DataBase接続(Close)
            SQLcon.Dispose()
            SQLcon = Nothing
            T7tbl.Dispose()
            T7tbl = Nothing

        Catch ex As Exception
            Master.output(C_MESSAGE_NO.DB_ERROR, C_MESSAGE_TYPE.ABORT, "T0007_KINTAI SELECT")
            CS0011LOGWRITE.INFSUBCLASS = "MAIN"                         'SUBクラス名
            CS0011LOGWRITE.INFPOSI = "DB:T0007_KINTAI Select"          '
            CS0011LOGWRITE.NIWEA = C_MESSAGE_TYPE.ABORT                                  '
            CS0011LOGWRITE.TEXT = ex.ToString()
            CS0011LOGWRITE.MESSAGENO = C_MESSAGE_NO.DB_ERROR
            CS0011LOGWRITE.CS0011LOGWrite()                             'ログ出力
            oRtn = C_MESSAGE_NO.DB_ERROR
            Exit Sub
        End Try

    End Sub

