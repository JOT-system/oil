            L0002row("CONTWORKTIMERATIO") = 0
            L0002row("CONTFUEL") = wCONT_NACKYUYU
            L0002row("CONTFUELRATIO") = 0
            L0002row("CONTDAISU") = 0
            L0002row("CONTDAISURATIO") = 0
