                SQLStr &=
                          " ORDER BY" _
                        & "    ISNULL(OIT0003.ACTUALLODDATE, OIT0002.LODDATE)" _
                        & " ,  RIGHT('00' + OIT0003.SHIPORDER, 2)" _
                        & " ,  CASE OIT0003.OILCODE WHEN '" + BaseDllConst.CONST_HTank + "' THEN 1 ELSE 2 END" _
