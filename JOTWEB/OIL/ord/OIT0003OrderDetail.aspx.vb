            Exit Sub
        End If

        '(予定)受入日 と　(予定)空車着日を比較
        iresult = Date.Parse(Me.TxtAccDate.Text).CompareTo(Date.Parse(Me.TxtEmparrDate.Text))
        If iresult = 1 Then
