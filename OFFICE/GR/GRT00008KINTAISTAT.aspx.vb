    ''' <summary>
    ''' 承認履歴テーブル強制更新処理
    ''' </summary>
    ''' <remarks></remarks>
    Protected Sub T00009tbl_update(ByVal iSTAFFCODE As String, ByRef oRtn As String)

        oRtn = C_MESSAGE_NO.NORMAL

        For i As Integer = 0 To T00008tbl.Rows.Count - 1
            Dim T8row As DataRow = T00008tbl.Rows(i)
            If CType(WF_Repeater.Items(i).FindControl("WF_Rep_CheckBox"), System.Web.UI.WebControls.CheckBox).Checked = True Then
                If T8row("LIMITFLG") = "0" Then
                    CS0048Apploval.I_CAMPCODE = work.WF_SEL_CAMPCODE.Text
                    CS0048Apploval.I_SUBCODE = T8row("ORGCODE")
                    If iSTAFFCODE = "" Then
                        CS0048Apploval.I_STAFFCODE = CS0050SESSION.USERID
                    Else
                        CS0048Apploval.I_STAFFCODE = iSTAFFCODE
                    End If
                    CS0048Apploval.I_UPDUSER = Master.USERID
                    CS0048Apploval.I_UPDTERMID = Master.USERTERMID
                    CS0048Apploval.CS0048setAllApproval()
                    If CS0048Apploval.O_ERR <> "00000" AndAlso CS0048Apploval.O_ERR <> "99999" Then
                        Master.output(CS0048Apploval.O_ERR, C_MESSAGE_TYPE.ABORT, "T0009_APPROVALHIST")
                        oRtn = CS0048Apploval.O_ERR
                        Exit Sub
                    End If
                    T8row("UNAPPROVED1") = "0"
                    T8row("UNAPPROVED2") = "0"
                    T8row("CHECKBOX") = "FALSE"
                End If
            End If
        Next

    End Sub

