    ''' <param name="sender"></param>
    ''' <param name="e"></param>
    ''' <remarks></remarks>
    Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs) Handles Me.Load

        Try
            '■■■ 作業用データベース設定 ■■■
            T00008ds = New DataSet()                                    '初期化
            T00008tbl = T00008ds.Tables.Add("T00008TBL")
            T00007tbl = T00008ds.Tables.Add("T00007tbl")
            L0001tbl = T00008ds.Tables.Add("L0001tbl")
            L0002tbl = T00008ds.Tables.Add("L0002tbl")
            L0003tbl = T00008ds.Tables.Add("L0003tbl")
            L0004tbl = T00008ds.Tables.Add("L0004tbl")
            L0005tbl = T00008ds.Tables.Add("L0005tbl")
            MA002tbl = T00008ds.Tables.Add("MA002tbl")

            WF_UNAPPROVED.Value = ""
            WF_UNAPPLIED.Value = ""
            WF_NGREC.Value = ""

            If IsPostBack Then
                '■■■ 各ボタン押下処理 ■■■
                If Not String.IsNullOrEmpty(WF_ButtonClick.Value) Then
                    Select Case WF_ButtonClick.Value
                        '********* ヘッダ部 *********
                        Case "WF_ButtonALLSELECT"
                            WF_ButtonALLSELECT_Click()
                        Case "WF_ButtonALLCANCEL"
                            WF_ButtonALLCANCEL_Click()
                        Case "WF_ButtonLIMIT"
                            WF_ButtonLIMIT_Click()
                        Case "WF_ButtonRELEASE"
                            WF_ButtonRELEASE_Click()
                        Case "WF_ButtonJOURNAL"
                            WF_ButtonJOURNAL_Click()
                        Case "WF_ButtonBUMON"
                            WF_ButtonBUMON_Click()
                        Case "WF_ButtonEND"
                            WF_ButtonEND_Click()
                            '********* 一覧 *********

                            '********* 右BOX *********
                        Case "WF_RadioButonClick"               '選択時
                            WF_RadioButon_Click()
                        Case "WF_MEMOChange"                    'メモ欄変更時
                            WF_MEMO_Change()

                            '********* その他はMasterPageで処理 *********
                        Case Else
                    End Select

                End If
            Else
                '〇初期化処理
                Initialize()
            End If

        Finally

            '○Close
            For Each tbl As DataTable In T00008ds.Tables
                If Not IsNothing(tbl) Then
                    tbl.Dispose()
                    tbl = Nothing
                End If
            Next
            T00008ds.Dispose()
            T00008ds.Clear()
            T00008ds = Nothing
        End Try

    End Sub

    ''' <summary>
    ''' 初期化処理
    ''' </summary>
    ''' <remarks></remarks>
    Protected Sub Initialize()
        '○初期値設定
        Master.MAPID = GRT00008WRKINC.MAPID
        '〇ヘルプ無
        Master.dispHelp = False
        '〇ドラックアンドドロップOFF
        Master.eventDrop = False

        '○Grid情報保存先のファイル名
        Master.createXMLSaveFile()

        '左Boxへの値設定

        '右Boxへの値設定
        rightview.resetindex()
        rightview.MAPID = Master.MAPID
        rightview.MAPVARI = Master.MAPvariant
        rightview.COMPCODE = work.WF_SEL_CAMPCODE.Text
        rightview.PROFID = Master.PROF_REPORT
        rightview.Initialize(WW_DUMMY)
        rightview.selectIndex(GRIS0004RightBox.RIGHT_TAB_INDEX.LS_MEMO_LIST)


        '■■■ 画面モード（更新・参照）設定  ■■■
        '総務部所属の場合、「締解除」、「ジャーナル作成」ボタンを有効にする
        If T0008COM.isGeneralAffair(work.WF_SEL_CAMPCODE.Text, Master.USER_ORG, WW_RTN_SW) Then
            If Master.MAPpermitcode = C_PERMISSION.UPDATE Then
                WF_MAPpermitcode.Value = "TRUE"
            Else
                WF_MAPpermitcode.Value = "FALSE"
            End If
        Else
            WF_MAPpermitcode.Value = "FALSE"
        End If

        If Master.MAPpermitcode = C_PERMISSION.UPDATE Then
            WF_MAPpermitcode2.Value = "TRUE"
        Else
            WF_MAPpermitcode2.Value = "FALSE"
        End If

        'ヘッダ表示
        If IsDate(work.WF_SEL_LIMITYM.Text) Then
            WF_LIMITYY.Text = CDate(work.WF_SEL_LIMITYM.Text).ToString("yyyy")
            WF_LIMITMM.Text = CDate(work.WF_SEL_LIMITYM.Text).ToString("MM")
        Else
            WF_LIMITYY.Text = ""
            WF_LIMITMM.Text = ""
        End If


        '■■■ T00008tbl初期設定処理 ■■■
        T00008tbl_init()

        '■■■ Detail初期設定 ■■■
        Repeater_init()

        '■■■ T00008tblデータ保存 ■■■
        If Master.SaveTable(T00008tbl) <> True Then Exit Sub

    End Sub


