            CS0011LOGWRITE.CS0011LOGWrite()                             'ログ出力
            oRtn = C_MESSAGE_NO.FILE_IO_ERROR
            Exit Sub

        End Try

    End Sub

    ''' <summary>
    ''' 時間変換（分→時分、１０分単位で切り上げ）
    ''' </summary>
    ''' <param name="iParm">分</param>
    ''' <returns>時分</returns>
    ''' <remarks>１０分単位切り上げ</remarks>
    Function formatHHHMM(ByVal iParm As Integer) As String

        '初期値、時間、分換算した結果をそのまま設定
        Dim WW_HHMM As Integer = Int(iParm / 60) * 100 + iParm Mod 60
        Dim WW_MOD As Integer = iParm Mod 60

        If WW_MOD > 50 Then
            '時間+1、0分
            WW_HHMM = (Int(iParm / 60) + 1) * 100 + 0
        ElseIf WW_MOD > 40 Then
            '時間、50分
            WW_HHMM = Int(iParm / 60) * 100 + 50
        ElseIf WW_MOD > 30 Then
            '時間、40分
            WW_HHMM = Int(iParm / 60) * 100 + 40
        ElseIf WW_MOD > 20 Then
            '時間、30分
            WW_HHMM = Int(iParm / 60) * 100 + 30
        ElseIf WW_MOD > 10 Then
            '時間、20分
            WW_HHMM = Int(iParm / 60) * 100 + 20
        ElseIf WW_MOD > 0 Then
            '時間、10分
            WW_HHMM = Int(iParm / 60) * 100 + 10
        End If

        formatHHHMM = Format(WW_HHMM, "00000")
    End Function

    ''' <summary>
    ''' 時間変換（分→時分、分四捨五入）
    ''' </summary>
    ''' <param name="iParm">分</param>
    ''' <returns>時分</returns>
    ''' <remarks>分単位四捨五入</remarks>
    Function formatHHHMM5(ByVal iParm As Integer) As String

        '初期値、時間、分換算した結果をそのまま設定
        Dim WW_HHMM As Integer = Int(iParm / 60) * 100 + iParm Mod 60
        Dim WW_MOD As Integer = iParm Mod 60

        If WW_MOD >= 55 And WW_MOD <= 59 Then
            '時間+1、0分
            WW_HHMM = (Int(iParm / 60) + 1) * 100 + 0
        ElseIf WW_MOD >= 45 And WW_MOD <= 54 Then
            '時間、50分
            WW_HHMM = Int(iParm / 60) * 100 + 50
        ElseIf WW_MOD >= 35 And WW_MOD <= 44 Then
            '時間、40分
            WW_HHMM = Int(iParm / 60) * 100 + 40
        ElseIf WW_MOD >= 25 And WW_MOD <= 34 Then
            '時間、30分
            WW_HHMM = Int(iParm / 60) * 100 + 30
        ElseIf WW_MOD >= 15 And WW_MOD <= 24 Then
            '時間、20分
            WW_HHMM = Int(iParm / 60) * 100 + 20
        ElseIf WW_MOD >= 5 And WW_MOD <= 14 Then
            '時間、10分
            WW_HHMM = Int(iParm / 60) * 100 + 10
        ElseIf WW_MOD >= 0 And WW_MOD <= 4 Then
            '時間、00分
            WW_HHMM = Int(iParm / 60) * 100 + 0
        End If

        formatHHHMM5 = Format(WW_HHMM, "00000")
    End Function

