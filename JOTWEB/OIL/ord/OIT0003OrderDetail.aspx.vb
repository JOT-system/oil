                O_RTN = "ERR"
                Continue For
            Else
                OIT0003row("ORDERINFO") = ""
                OIT0003row("ORDERINFONAME") = ""
            End If
        Next
        '○ 画面表示データ保存
        Master.SaveTable(OIT0003tbl)
        If O_RTN = "ERR" Then
            Master.Output(C_MESSAGE_NO.OIL_DATE_VALIDITY_ERROR, C_MESSAGE_TYPE.ERR, "(一覧)積込日 > (予定)発日", needsPopUp:=True)
