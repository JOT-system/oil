                    If Me.TxtActualDepDate.Text <> "" Then
                        '◯ (予定)(発日) = (実績)発日
                        If Me.TxtDepDate.Text = Me.TxtActualDepDate.Text Then
                            '◯ (予定)(発日) = (予定)空車着日
                            If Me.TxtDepDate.Text = Me.TxtEmparrDate.Text Then
                                '(予定)空車着日の日付を設定する。
                                PARA03.Value = Me.TxtEmparrDate.Text

                            Else
                                '(予定)空車着日(前日)の日付を設定する。
                                PARA03.Value = dtEDbefore

                            End If
                        Else
                            '(予定)空車着日の日付を設定する。
                            PARA03.Value = Me.TxtEmparrDate.Text

