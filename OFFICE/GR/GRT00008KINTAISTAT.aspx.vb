    ''' <summary>
    ''' 給与ジャーナルボタン処理
    ''' </summary>
    ''' <remarks></remarks>
    Protected Sub WF_ButtonJOURNAL_Click()

        Dim WW_Dir As String = ""
        Dim WW_TEMPDir As String = ""
        Dim WW_ListSTAFFKBN As ListBox = New ListBox
        WW_ERRLIST_ALL = New List(Of String)

        '■ 作業用フォルダ・作業用ファイルの事前操作
        Try
            '○　時間外対象外従業員区分ListBox設定
            GS0007FIXVALUElst.CAMPCODE = work.WF_SEL_CAMPCODE.Text
            GS0007FIXVALUElst.CLAS = "T0009_STAFFKBN"
            GS0007FIXVALUElst.LISTBOX1 = WW_ListSTAFFKBN
            GS0007FIXVALUElst.GS0007FIXVALUElst()
            If isNormal(GS0007FIXVALUElst.ERR) Then
                WW_ListSTAFFKBN = GS0007FIXVALUElst.LISTBOX1
            Else
                Master.output(GS0007FIXVALUElst.ERR, C_MESSAGE_TYPE.ABORT)
                Exit Sub
            End If

            '○ 作業フォルダ存在確認＆作成(C:\apple\files\TEXTWORK)
            WW_Dir = Path.Combine(CS0050SESSION.UPLOAD_PATH, "TEXTWORK")
            If System.IO.Directory.Exists(WW_Dir) Then
            Else
                System.IO.Directory.CreateDirectory(WW_Dir)
            End If

            '○ ファイル格納フォルダ存在確認＆作成(C:\apple\files\TEXTWORK\[USER])　＆　前回処理ファイル削除
            WW_Dir = Path.Combine(WW_Dir, Master.USERID)
            If System.IO.Directory.Exists(WW_Dir) Then
                'ファイル格納フォルダ内不要ファイル削除(すべて削除)
                For Each tempFile As String In System.IO.Directory.GetFiles(WW_Dir, "*.*")
                    System.IO.File.Delete(tempFile)
                Next
            Else
                System.IO.Directory.CreateDirectory(WW_Dir)
            End If

            '○ TEMPフォルダ存在確認＆作成(C:\apple\files\TEXTWORK\TEMP\[USER])　＆　前回処理ファイル削除
            WW_TEMPDir = Path.Combine(CS0050SESSION.UPLOAD_PATH, "TEXTWORK", "TEMP", Master.USERID)
            If System.IO.Directory.Exists(WW_TEMPDir) Then
                'TEMPフォルダ内不要ファイル削除(すべて削除)
                For Each tempFile As String In System.IO.Directory.GetFiles(WW_TEMPDir, "*.*")
                    System.IO.File.Delete(tempFile)
                Next
            Else
                System.IO.Directory.CreateDirectory(WW_TEMPDir)
            End If

        Catch ex As Exception
            Master.output(C_MESSAGE_NO.SYSTEM_ADM_ERROR, C_MESSAGE_TYPE.ABORT)
            Exit Sub
        End Try

        '■■■ T00008tbl復元処理 ■■■
        If Master.RecoverTable(T00008tbl) <> True Then Exit Sub

        '----------------------------------------
        '給与ジャーナル作成
        '----------------------------------------
        Dim WW_ERR_CODE As String = C_MESSAGE_NO.NORMAL
        Dim WW_FileOut As String = "OFF"
        WW_Dir = Path.Combine(CS0050SESSION.UPLOAD_PATH, "TEXTWORK", Master.USERID, "KYUYO")

        For i As Integer = 0 To WF_Repeater.Items.Count - 1
            If CType(WF_Repeater.Items(i).FindControl("WF_Rep_CheckBox"), System.Web.UI.WebControls.CheckBox).Checked = True Then
                Dim T00008row = T00008tbl.Rows(i)

                If T00008row("LIMITFLG") = "1" Then
                    '月合計レコード取得
                    T00007tblget(T00008row("ORGCODE"), WW_ERRCODE)
                    If Not isNormal(WW_ERRCODE) Then
                        Exit Sub
                    End If

                    '重複チェック
                    Dim WW_MSG As String = ""
                    T0007COM.T0007_DuplCheck(T00007tbl, WW_MSG, WW_ERRCODE)
                    If Not isNormal(WW_ERRCODE) Then
                        Master.output(WW_ERRCODE, C_MESSAGE_TYPE.ABORT)
                        rightview.addErrorReport(WW_MSG)
                        WW_ERR_CODE = WW_ERRCODE
                    End If

                    '給与ジャーナル作成
                    Dim P_DIR As String = Path.Combine(WW_Dir, T00008row("OUTDIR"))
                    Select Case work.WF_SEL_CAMPCODE.Text
                        Case CONST_CAMP_ENEX
                            'ENEX
                            KyuyoFileOutPut(T00008row("ORGCODE"), P_DIR, WW_ListSTAFFKBN, WW_ERRCODE)
                        Case CONST_CAMP_KNK
                            '近石
                            KyuyoFileOutPutKNK(T00008row("ORGCODE"), P_DIR, WW_ListSTAFFKBN, WW_ERRCODE)
                        Case CONST_CAMP_NJS
                            'NJS
                            KyuyoFileOutPutNJS(T00008row("ORGCODE"), P_DIR, WW_ListSTAFFKBN, WW_ERRCODE)
                        Case CONST_CAMP_JKT
                            'JKT
                            KyuyoFileOutPutJKT(T00008row("ORGCODE"), P_DIR, WW_ListSTAFFKBN, WW_ERRCODE)
                    End Select
                    If Not isNormal(WW_ERRCODE) Then
                        Exit Sub
                    End If

                    WW_FileOut = "ON"
                End If

            End If
        Next

        '重複チェックエラーの場合、終了
        If Not isNormal(WW_ERR_CODE) Then
            Exit Sub
        End If

        Try
            If WW_FileOut = "ON" Then
                '■ ダウンロード
                '○ 圧縮実行
                Dim WW_Dir2 As String = Path.Combine(CS0050SESSION.UPLOAD_PATH(), "TEXTWORK", "TEMP", Master.USERID, JOURNAL_FILE.KJZIP)
                ZipFile.CreateFromDirectory(WW_Dir, WW_Dir2, CompressionLevel.Fastest, True, Encoding.GetEncoding("sjis"))
                Dim uri As UriBuilder = New UriBuilder() With {
                    .Scheme = HttpContext.Current.Request.Url.Scheme,
                    .Host = HttpContext.Current.Request.Url.Host,
                    .Path = "TEXT/TEMP/" & Master.USERID & "/" & JOURNAL_FILE.KJZIP
                }
                WF_DownURL.Value = uri.Uri.ToString
                '○ ダウンロード処理へ遷移
                ClientScript.RegisterStartupScript(Me.GetType(), "key", "Z_DownLoad()", True)

                'dir削除
                My.Computer.FileSystem.DeleteDirectory(WW_Dir,
                                                    FileIO.UIOption.OnlyErrorDialogs,
                                                    FileIO.RecycleOption.DeletePermanently)
            End If

        Catch ex As Exception
            Master.output(C_MESSAGE_NO.SYSTEM_ADM_ERROR, C_MESSAGE_TYPE.ABORT, "FILE操作で異常")
            CS0011LOGWRITE.INFSUBCLASS = "WF_ButtonJOURNAL_Click"       'SUBクラス名
            CS0011LOGWRITE.INFPOSI = "FILE操作で異常"                   '
            CS0011LOGWRITE.NIWEA = C_MESSAGE_TYPE.ABORT                                  '
            CS0011LOGWRITE.TEXT = ex.ToString()
            CS0011LOGWRITE.MESSAGENO = C_MESSAGE_NO.SYSTEM_ADM_ERROR
            CS0011LOGWRITE.CS0011LOGWrite()                             'ログ出力
            Exit Sub

        End Try

        If WW_FileOut = "ON" Then
            '------------------------------------------------
            'MA002_SHARYOA更新（車両ステータスのみ更新）
            '------------------------------------------------
            MA002_get(WW_ERRCODE)
            If Not isNormal(WW_ERRCODE) Then
                Exit Sub
            End If

            MA002_UPDATE(WW_ERRCODE)
            If Not isNormal(WW_ERRCODE) Then
                Exit Sub
            End If
        End If

        '■■■ Detail初期設定 ■■■
        Repeater_init()

        If WW_ERRLIST_ALL.Count > 0 Then
            Master.output(C_MESSAGE_NO.BOX_ERROR_EXIST, C_MESSAGE_TYPE.ERR)
        Else
            Master.output(C_MESSAGE_NO.NORMAL, C_MESSAGE_TYPE.INF)
        End If

    End Sub

