                End If

                If work.WF_SEL_ORGCODE.Text <> "" Then
                    If work.WF_SEL_ORGCODE.Text = SQLdr("ORGCODE") Then
                        T00008row("SELECT") = 1   '1:表示
                    Else
                        T00008row("SELECT") = 0   '0:非表示
                    End If
                Else
                    T00008row("SELECT") = 1   '1:表示
                End If
                T00008row("HIDDEN") = 0   '0:表示

