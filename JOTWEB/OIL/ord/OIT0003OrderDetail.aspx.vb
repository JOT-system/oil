                Dim WW_DATE As Date
                Try
                    Date.TryParse(leftview.WF_Calendar.Text, WW_DATE)
                    If WW_DATE < C_DEFAULT_YMD Then
                        Me.TxtLoadingDate.Text = ""
                    Else
                        Me.TxtLoadingDate.Text = WW_DATE.ToString("yyyy/MM/dd")
                        'Me.TxtLoadingDate.Text = leftview.WF_Calendar.Text
