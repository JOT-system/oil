            L0002row("OTHRFUELRATIO") = 0
            L0002row("OTHRDAISU") = 0
            L0002row("OTHRDAISURATIO") = 0
            L0002row("OTHROTHER") = 0
            L0002row("OTHROTHERRATIO") = 0
            L0002row("OTHRJIDISTANCE") = wOTHR_NACJITTLDISTANCE
            L0002row("OTHRKUDISTANCE") = wOTHR_NACKUTTLDISTANCE

            L0002row("NOTE1") = 0
            L0002row("NOTE2") = 0
            L0002row("NOTE3") = 0
            L0002row("NOTE4") = 0
            L0002row("NOTE5") = 0
            L0002row("NOTE6") = 0

            '--------------------
            '走行率
            '--------------------
            Dim TTL As Long = L0002row("OILDISTANCE") +
                              L0002row("HPRSDISTANCE") +
                              L0002row("CHEMDISTANCE") +
                              L0002row("CONTDISTANCE") +
                              L0002row("OTHRDISTANCE")

            If TTL <> 0 Then
                L0002row("OILDISTANCERATIO") = Int((L0002row("OILDISTANCE") / TTL + 0.005) * 100)
                L0002row("HPRSDISTANCERATIO") = Int((L0002row("HPRSDISTANCE") / TTL + 0.005) * 100)
                L0002row("CHEMDISTANCERATIO") = Int((L0002row("CHEMDISTANCE") / TTL + 0.005) * 100)
                L0002row("CONTDISTANCERATIO") = Int((L0002row("CONTDISTANCE") / TTL + 0.005) * 100)
                L0002row("OTHRDISTANCERATIO") = Int((L0002row("OTHRDISTANCE") / TTL + 0.005) * 100)
