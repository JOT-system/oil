                        PARA32 = SQLcmd.Parameters.Add("@P32", System.Data.SqlDbType.NVarChar)  '積込出線順
                        PARA33 = SQLcmd.Parameters.Add("@P33", System.Data.SqlDbType.NVarChar)  '積込入線順
