                wOTHR_NACKUTTLTIME = 0
                wOTHR_NACJITTLDISTANCE = wOTHR_NACJITTLDISTANCE + L0001row("NACJITTLDISTANCE")
                wOTHR_NACKUTTLDISTANCE = wOTHR_NACKUTTLDISTANCE + L0001row("NACKUTTLDISTANCE")
            End If

        Next

        L0002tbl = New DataTable
        L0002tbl_ColumnsAdd(L0002tbl)

        If L0001tbl.Rows.Count > 0 Then

            Dim L0002row = L0002tbl.NewRow
