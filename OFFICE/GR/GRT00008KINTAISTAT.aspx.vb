               & "  LEFT JOIN M0006_STRUCT F                                            " _
               & "         ON  F.CAMPCODE = A.CAMPCODE                                  " _
               & "        and  F.OBJECT   = 'ORG'                                       " _
               & "        and  F.STRUCT   = '勤怠管理組織'                              " _
               & "        and  F.CODE     = A.ORGCODE                                   " _
               & "        and  F.STYMD   <= @YMD2                                       " _
               & "        and  F.ENDYMD  >= @YMD2                                       " _
               & "        and  F.DELFLG  <> '1'                                         " _
               & "  WHERE      A.CAMPCODE = @CAMPCODE                                   " _
               & "        and  A.LIMITYM  = @LIMITYM                                    " _
