    ''' <summary>
    ''' 勤怠締ボタン処理
    ''' </summary>
    ''' <remarks></remarks>
    Protected Sub WF_ButtonLIMIT_Click()

        '■■■ エラーレポート準備 ■■■
        rightview.setErrorReport("")

        '■■■ T00008tbl復元処理 ■■■
        If Master.RecoverTable(T00008tbl) <> True Then Exit Sub

        T00008tbl.AcceptChanges()

        '■■■  DetailBoxをT00008tblへ退避 ■■■
        DetailBoxToT00008tbl("LIMIT")

        '--------------------------------
        '残業承認済の場合、締処理が可能
        '--------------------------------

        '■■■  L0002_BUMON更新 、L0003_SUMMAYN更新■■■
        For Each T00008row As DataRow In T00008tbl.Rows
            If T00008row.RowState = DataRowState.Modified Then
                'データに変更が存在するレコードのみ、以下処理を行う

                If T00008row("ORGCODE") <> Master.USER_ORG And T00008row("LIMITKBN") = "支店" Then
                    '支店で営業所分を締める場合（仮締→締）、サマリーテーブルは仮締時に営業所で作成済のためスキップする
                    Continue For
                End If

                '------------------------------------------------
                'L0002_BUMON更新
                '------------------------------------------------
                L0001_get(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                L0001_SUM()

                L0002_UPDATE(WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                '------------------------------------------------
                'L0003_SUMMARYN更新
                '------------------------------------------------
                L0003tbl_get(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                'L0003SUMMARYN削除（論理削除 DELFLG='1'）
                L0003SUMMARYN_DELETE(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                'L0003SUMMARYN作成（論理削除→INPUTに存在するなら物理削除→INSERT）
                L0003SUMMARYN_UPDATE(WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                '------------------------------------------------
                'L0004_SUMMARYK更新
                '------------------------------------------------
                L0004tbl_get(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                'L0004SUMMARYK削除（論理削除 DELFLG='1'）
                L0004SUMMARYK_DELETE(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                'L0004SUMMARYK作成（論理削除→INPUTに存在するなら物理削除→INSERT）
                L0004SUMMARYK_UPDATE(WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                '------------------------------------------------
                'L0005_SUMMARYY更新
                '------------------------------------------------
                L0005tbl_get(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                'L0005SUMMARYN削除（論理削除 DELFLG='1'）
                L0005SUMMARYN_DELETE(T00008row("ORGCODE"), WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

                'L0005SUMMARYN作成（論理削除→INPUTに存在するなら物理削除→INSERT）
                L0005SUMMARYN_UPDATE(WW_ERRCODE)
                If Not isNormal(WW_ERRCODE) Then
                    Exit Sub
                End If

            End If
        Next


        '■■■  T0008_KINTAISTAT更新 ■■■
        T0008_UPDATE(WW_ERRCODE)
        If Not isNormal(WW_ERRCODE) Then
            Exit Sub
        End If


        '■■■ Detail初期設定 ■■■
        '未承認がある場合、JavaScript（メッセージボックス）を表示する
        For i As Integer = 0 To WF_Repeater.Items.Count - 1
            If CType(WF_Repeater.Items(i).FindControl("WF_Rep_CheckBox"), System.Web.UI.WebControls.CheckBox).Checked = True Then
                If T00008tbl.Rows(i)("UNAPPROVED2") > "0" Then
                    T00008tbl.Rows(i)("CHECKBOX") = "TRUE"
                End If
            End If
        Next

        Repeater_init()

        'メッセージ表示
        If String.IsNullOrEmpty(rightview.getErrorReport()) Then
            Master.output(C_MESSAGE_NO.DATA_UPDATE_SUCCESSFUL, C_MESSAGE_TYPE.INF)
        Else
            Master.output(C_MESSAGE_NO.BOX_ERROR_EXIST, C_MESSAGE_TYPE.ERR)
        End If
    End Sub

