            CType(WF_Repeater.Items(i).FindControl("WF_Rep_LINECNT"), System.Web.UI.WebControls.Label).Text = Val(T00008tbl.Rows(i)("LINECNT")) + 1
            'チェックボックス
            If T00008tbl.Rows(i)("CHECKBOX") = "TRUE" Then
                CType(WF_Repeater.Items(i).FindControl("WF_Rep_CheckBox"), System.Web.UI.WebControls.CheckBox).Checked = True
            Else
