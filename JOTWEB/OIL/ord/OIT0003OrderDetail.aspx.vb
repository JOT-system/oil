                            WW_CheckMES1 = "荷受人(油槽所)受入油種NG。"
                            WW_CheckMES2 = C_MESSAGE_NO.OIL_CONSIGNEE_OILCODE_NG
                            WW_CheckListERR(WW_CheckMES1, WW_CheckMES2, OIT0003row)
                            O_RTN = "ERR"
                            O_VALUE = OIT0003ChkDrow("CONSIGNEENAME")
                            'Exit Sub

                            '受注明細TBLの受注情報を更新
