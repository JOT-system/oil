                If OIT0003tab3row("OTTRANSPORTFLG") = "on" Then
                    PARA28.Value = "1"
                Else
                    PARA28.Value = "2"
                End If
                '### 20200717 END  ((全体)No112対応) #####################################

                PARA18.Value = Date.Now
                PARA19.Value = Master.USERID
                PARA20.Value = Master.USERTERMID
                PARA21.Value = C_DEFAULT_YMD

