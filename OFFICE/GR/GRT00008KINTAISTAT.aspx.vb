                  L0002row("HPRSDISTANCERATIO") +
                  L0002row("CHEMDISTANCERATIO") +
                  L0002row("CONTDISTANCERATIO") +
                  L0002row("OTHRDISTANCERATIO")

            Dim SA As Long = 0

            If TTL <> 0 Then
                SA = 100 - TTL

                If SA <> 0 Then
                    '石油
                    If L0002row("OILDISTANCERATIO") <> 0 Then
                        L0002row("OILDISTANCERATIO") = Val(L0002row("OILDISTANCERATIO")) + SA
