            End If
            '所属部署コード
            CType(WF_Repeater.Items(i).FindControl("WF_Rep_ORGCODE"), System.Web.UI.WebControls.Label).Text = T00008tbl.Rows(i)("ORGCODE")
            '所属部署名称
            CType(WF_Repeater.Items(i).FindControl("WF_Rep_ORGCODENAME"), System.Web.UI.WebControls.Label).Text = T00008tbl.Rows(i)("ORGCODENAME")
            '締フラグ名称
            CType(WF_Repeater.Items(i).FindControl("WF_Rep_LIMITFLGNAME"), System.Web.UI.WebControls.Label).Text = T00008tbl.Rows(i)("LIMITFLGNAME")
