            & " , ISNULL(RTRIM(TMP0001.ORDERNO), '')                            AS ORDERNO" _
            & " , ISNULL(RTRIM(TMP0001.DETAILNO), '')                           AS DETAILNO" _
            & " , ISNULL(RTRIM(TMP0001.SHIPPERSCODE), '')                       AS SHIPPERSCODE" _
