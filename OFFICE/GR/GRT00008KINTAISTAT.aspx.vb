    ''' <summary>
    ''' 時刻取得処理
    ''' </summary>
    ''' <remarks></remarks>
    Function formatHHMM(ByVal iParm As Integer) As String
        Dim WW_HHMM As Integer = 0
        Dim WW_ABS As Integer = System.Math.Abs(iParm)

        WW_HHMM = Int(WW_ABS / 60) * 100 + WW_ABS Mod 60
        If iParm < 0 Then
            WW_HHMM = WW_HHMM * -1
        End If
        formatHHMM = Format(WW_HHMM, "0#:##")
    End Function

    ''' <summary>
    ''' MA002_SHARYOデータ取得処理
    ''' </summary>
    ''' <remarks></remarks>
    Private Sub MA002_get(ByRef oRTN As String)

        '○初期クリア
        oRTN = C_MESSAGE_NO.NORMAL

        Try
            'カラム設定
            MA002SHARYOA_ColumnsAdd(MA002tbl)

            Dim SQLcon = CS0050SESSION.getConnection
            Dim SQLcmd As New SqlCommand
            SQLcon.Open() 'DataBase接続(Open)

            '検索SQL文
            Dim SQLStr As String =
                 "SELECT " _
               & "       isnull(rtrim(A.CAMPCODE),'') as CAMPCODE                 , " _
               & "       isnull(rtrim(A.MANGMORG),'') as MANGMORG                 , " _
               & "       isnull(rtrim(A.MANGSORG),'') as MANGSORG                 , " _
               & "       isnull(rtrim(A.SHARYOTYPE),'') as SHARYOTYPE             , " _
               & "       isnull(rtrim(A.TSHABAN),'') as TSHABAN                   , " _
               & "       A.STYMD as STYMD                                         , " _
               & "       A.ENDYMD as ENDYMD                                       , " _
               & "       isnull(rtrim(A.MANGOILTYPE),'') as MANGOILTYPE           , " _
               & "       isnull(rtrim(A.MANGOWNCODE),'') as MANGOWNCODE           , " _
               & "       isnull(rtrim(A.MANGPROD1),'') as MANGPROD1               , " _
               & "       isnull(rtrim(A.MANGPROD2),'') as MANGPROD2               , " _
               & "       isnull(rtrim(A.MANGOWNCONT),'') as MANGOWNCONT           , " _
               & "       isnull(A.MANGSHAFUKU,'0') as MANGSHAFUKU                 , " _
               & "       isnull(rtrim(A.MANGSUPPL),'') as MANGSUPPL               , " _
               & "       isnull(A.MANGTTLDIST,'0') as MANGTTLDIST                 , " _
               & "       isnull(rtrim(A.BASELEASE),'') as BASELEASE               , " _
               & "       isnull(rtrim(A.SHARYOSTATUS),'') as SHARYOSTATUS         , " _
               & "      (select count(*) from TA001_SHARYOSTAT B                    " _
               & "       where B.CAMPCODE   = A.CAMPCODE                            " _
               & "         and B.SHARYOTYPE = A.SHARYOTYPE                          " _
               & "         and B.TSHABAN    = A.TSHABAN                             " _
               & "         and B.KADOYMD   >= @P04                                  " _
               & "         and B.DELFLG    <> '1'                                   " _
               & "      ) as KADOCNT                                              , " _
               & "       '0' as UPDFLG                                            , " _
               & "       isnull(rtrim(A.DELFLG),'') as DELFLG                       " _
               & " FROM MA002_SHARYOA A " _
               & " WHERE   A.CAMPCODE    = @P01 " _
               & "   and   A.SHARYOTYPE in ('A','C') " _
               & "   and   A.STYMD      <= @P03 " _
               & "   and   A.ENDYMD     >= @P02 " _
               & "   and   A.DELFLG     <> '1' " _
               & " ORDER BY A.SHARYOTYPE ASC, A.TSHABAN ASC, A.STYMD ASC "


            SQLcmd = New SqlCommand(SQLStr, SQLcon)
            Dim PARA01 As SqlParameter = SQLcmd.Parameters.Add("@P01", System.Data.SqlDbType.NVarChar)
            Dim PARA02 As SqlParameter = SQLcmd.Parameters.Add("@P02", System.Data.SqlDbType.Date)
            Dim PARA03 As SqlParameter = SQLcmd.Parameters.Add("@P03", System.Data.SqlDbType.Date)
            Dim PARA04 As SqlParameter = SQLcmd.Parameters.Add("@P04", System.Data.SqlDbType.Date)
            PARA01.Value = work.WF_SEL_CAMPCODE.Text
            PARA02.Value = work.WF_SEL_LIMITYM.Text & "/01"
            '終了日（月末）
            Dim dt As Date = CDate(work.WF_SEL_LIMITYM.Text & "/01")
            Dim WW_ENDDATE As String = dt.AddMonths(1).AddDays(-1).ToString("yyyy/MM/dd")
            PARA03.Value = WW_ENDDATE
            '終了日（３ヶ月前）
            Dim dt2 As Date = CDate(work.WF_SEL_LIMITYM.Text & "/01")
            Dim WW_3DOWNDATE As String = dt2.AddMonths(-2).ToString("yyyy/MM/dd")
            PARA04.Value = WW_3DOWNDATE


            SQLcmd.CommandTimeout = 300
            Dim SQLdr As SqlDataReader = SQLcmd.ExecuteReader()

            MA002tbl.Load(SQLdr)

            For Each MA2row As DataRow In MA002tbl.Rows
                If Val(MA2row("KADOCNT")) > 0 Then
                    If MA2row("SHARYOSTATUS") = "1" Then
                        'ステータス変更なし（更新しない）
                        MA2row("UPDFLG") = "0"
                    Else
                        '運行
                        MA2row("SHARYOSTATUS") = "1"
                        'ステータス変更あり（更新対象）
                        MA2row("UPDFLG") = "1"
                    End If

                Else
                    If MA2row("SHARYOSTATUS") = "1" Then
                        '未運行
                        MA2row("SHARYOSTATUS") = ""
                        'ステータス変更あり（更新対象）
                        MA2row("UPDFLG") = "1"
                    Else
                        'ステータス変更なし（更新しない）
                        MA2row("UPDFLG") = "0"
                    End If
                End If
            Next

            SQLdr.Dispose() 'Reader(Close)
            SQLdr = Nothing
            SQLcmd.Dispose()
            SQLcmd = Nothing
            SQLcon.Close() 'DataBase接続(Close)
            SQLcon.Dispose()
            SQLcon = Nothing

        Catch ex As Exception
            Master.output(C_MESSAGE_NO.DB_ERROR, C_MESSAGE_TYPE.ABORT, "MA002_SHARYOA SELECT")
            CS0011LOGWRITE.INFSUBCLASS = "MAIN"                         'SUBクラス名
            CS0011LOGWRITE.INFPOSI = "DB:MA002_SHARYOA Select"          '
            CS0011LOGWRITE.NIWEA = C_MESSAGE_TYPE.ABORT                                  '
            CS0011LOGWRITE.TEXT = ex.ToString()
            CS0011LOGWRITE.MESSAGENO = C_MESSAGE_NO.DB_ERROR
            CS0011LOGWRITE.CS0011LOGWrite()                             'ログ出力
            oRTN = C_MESSAGE_NO.DB_ERROR
            Exit Sub
        End Try

    End Sub


