            'コンテナ（油種="04"）
            'If L0001row("NACOILTYPE_1") = "04" Then
            'コンテナ（車両油種="04"）
            If WW_OILTYPE = "04" Then
                wCONT_NACKYUYU = wCONT_NACKYUYU + L0001row("NACKYUYU")
                wCONT_NACJITTLETIME = wCONT_NACJITTLETIME + L0001row("NACTTLWORKTIME")
                wCONT_NACKUTTLTIME = 0
                wCONT_NACJITTLDISTANCE = wCONT_NACJITTLDISTANCE + L0001row("NACJITTLDISTANCE")
