    ''' <remarks></remarks>
    Protected Sub Repeater_init()

        Dim WW_TEXT As String = ""

        '■■■ Detail変数設定 ■■■
        For i As Integer = 0 To T00008tbl.Rows.Count - 1
            If T00008tbl.Rows(i)("LIMITFLG") = "0" Then
                T00008tbl.Rows(i)("COLOR") = "background-color:#FFFF99" '薄い黄色
            ElseIf T00008tbl.Rows(i)("LIMITFLG") = "1" Then
                T00008tbl.Rows(i)("COLOR") = "background-color:#EEFFFF" '薄い青色
            Else
                T00008tbl.Rows(i)("COLOR") = "background-color:#FFA07A" 'ベージュ系
            End If
        Next

        Dim WW_TBLview As DataView = New DataView(T00008tbl)
        WF_Repeater.DataSource = WW_TBLview
        WF_Repeater.DataBind()  'Bind処理記述を行っていないので空行だけ作成される。

        For i As Integer = 0 To WF_Repeater.Items.Count - 1
